  recoded_variable <- paste0(variable, "r")
  reliability_df[[recoded_variable]] <- dplyr::recode(reliability_df[[variable]],
                                                      `1` = 5, `2` = 4, `3` = 3, `4` = 2, `5` = 1)
  attr(reliability_df[[recoded_variable]], "label") <- "Reversed"
}

